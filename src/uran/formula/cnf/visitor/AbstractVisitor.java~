package uran.formula.cnf.visitor;

import uran.formula.cnf.*;

public abstract class AbstractVisitor{
	
	/* methods for visit basic boolean circuits */

	public abstract int visit(BooleanVariable v);
	public abstract int[] visit(BinaryGate b);
	public abstract int[] visit(NotGate n);
	
}
