package uran.err;

public final class UnsatException extends AbstractException{
	
	private final int code=0x19;

	public UnsatException(){
		//super ("Couldn't find any models due to the unsatisfiability of the model.",code);
		super();
	}

	public UnsatException(String msg){
		super(msg);
	}
	
}
