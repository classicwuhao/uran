package uran.err;

public final class MissingFormulaException extends AbstractException{
	private final int code = 0x15;

	public MissingFormulaException(){
		//super ("formula are missing for this operation",code);
		super();
	}

	public MissingFormulaException(String msg){
		super(msg);
	}

	
}
